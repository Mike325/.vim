#############################################################################

snippet %- "%% ----..."
%% -----------------------------------------------------------------------------
endsnippet

snippet beg "\begin{} ... \end{}"
\begin{$1}
	$2
\end{`!p snip.rv = t[1]`}
$0
endsnippet

snippet inc "\include{}"
\include{$1}
endsnippet

snippet pkg "\usepackage{}"
\usepackage{$1}
endsnippet

snippet br "\pagebreak{}"
\pagebreak{}
$0
endsnippet

snippet spc "\vspace*{cm}"
\vspace*{${1:1}cm}
$0
endsnippet

snippet huge "{\huge ...}"
{\huge $1}
$0
endsnippet

# snippet bib "\bibliography{} ... \bibliographystyle{}"
# \bibliography{$1}
# \bibliographystyle{${2:IEEEtran}}
# $0
# endsnippet

snippet list "\begin{itemize}...\end{itemize}"
\begin{itemize}
	\item ${1:Item}
\end{itemize}
endsnippet

snippet figure "\begin{figure}...\end{figure}"
\begin{figure}[ht]
	\begin{center}
		\includegraphics[height=6cm]{${1:image.jpg}}
		\caption{${2:Caption}}
	\end{center}
\end{figure}
endsnippet

snippet bib "\bibitem{NAME}"
\bibitem{${1:id}}
	$0
endsnippet
