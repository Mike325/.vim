{
    "project.vim"            : { "type": "Project" },
    ".projections.json"      : { "type": "Projections" },
    ".gitignore"             : { "type": "Gitignore" },
    ".git/hooks/*"           : { "type": "GitHooks" },
    ".git/config"            : { "type": "Git" },
    ".git/info/*"            : { "type": "Git" },
    "README.md"              : { "type": "Readme" },
    "LICENSE"                : { "type": "License" },
    "clang-format"           : { "type": "ClangFormat" },
    "ycm_extra_conf.py"      : { "type": "ConfigYCM" },
    "init.vim"               : { "type": "Init", "alternate" : "ginit.vim" },
    "ginit.vim"              : { "type": "Ginit", "alternate": "init.vim" },
    "lua/*.lua"              : { "type": "Lua" },
    "lua/helper/*.lua"       : { "type": "LuaHelper" },
    "lua/host/*.lua"         : { "type": "LuaHost" },
    "rplugins/python3/*.py"  : { "type": "RPython3" },
    "rplugins/python/*.py"   : { "type": "RPython" },
    "python3/*.py"           : { "type": "Python3" },
    "python/*.py"            : { "type": "Python" },
    "syntax/*.vim"           : { "type": "Syntax" },
    "autoload/*.vim"         : { "type": "Autoload" },
    "autoload/host/*.vim"    : { "type": "HostAutoload" },
    "autoload/plugins/*.vim" : { "type": "PluginsAutoload" },
    "plugin/*.vim"           : { "type": "Plugin" },
    "ftplugin/*.vim"         : { "type": "FtPlugin" },
    "ftdetect/*.vim"         : { "type": "FtDetect" },
    "after/plugin/*.vim"     : { "type": "AfterPlugin" },
    "after/ftplugin/*.vim"   : { "type": "AfterFtplugin" },
    "UltiSnips/*.snippets"   : { "type": "UltiSnips" },
    "spell/*.add"            : { "type": "Spells" },
    "setting.toml"           : { "type": "Gonvim" },
    "skeletons/*"            : { "type": "Skeletons" }
}
